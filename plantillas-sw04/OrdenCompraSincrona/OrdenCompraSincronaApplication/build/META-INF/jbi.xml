<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<jbi xmlns="http://java.sun.com/xml/ns/jbi" xmlns:ns1="http://manufacturing.org/wsdl/purchase/bp1" xmlns:ns2="http://enterprise.netbeans.org/bpel/OrdenCompraSincrona/POService" xmlns:ns3="http://manufacturing.org/wsdl/inventory/bp1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.0" xsi:schemaLocation="http://java.sun.com/xml/ns/jbi ./jbi.xsd">
    <service-assembly>
        <identification>
            <name>OrdenCompraSincronaApplication</name>
            <description>Represents the Service Assembly of OrdenCompraSincronaApplication</description>
        </identification>
        <service-unit>
            <identification>
                <name>OrdenCompraSincronaApplication-OrdenCompraSincrona</name>
                <description>Represents this Service Unit</description>
            </identification>
            <target>
                <artifacts-zip>OrdenCompraSincrona.jar</artifacts-zip>
                <component-name>sun-bpel-engine</component-name>
            </target>
        </service-unit>
        <service-unit>
            <identification>
                <name>OrdenCompraSincronaApplication-sun-http-binding</name>
                <description>Represents this Service Unit</description>
            </identification>
            <target>
                <artifacts-zip>sun-http-binding.jar</artifacts-zip>
                <component-name>sun-http-binding</component-name>
            </target>
        </service-unit>
        <connections>
            <connection>
                <consumer endpoint-name="purchaseOrderPort" service-name="ns1:purchaseOrderService"/>
                <provider endpoint-name="purchaseService_myRole" service-name="ns2:Cliente"/>
            </connection>
            <connection>
                <consumer endpoint-name="inventoryServicePort" service-name="ns3:inventoryService"/>
                <provider endpoint-name="inventoryService_myRole" service-name="ns3:inventorySevicePLink"/>
            </connection>
            <connection>
                <consumer endpoint-name="inventoryService_partnerRole" service-name="ns2:requestInvetoryPLink"/>
                <provider endpoint-name="inventoryService_myRole" service-name="ns3:inventorySevicePLink"/>
            </connection>
        </connections>
    </service-assembly>
</jbi>
